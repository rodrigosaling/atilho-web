{
  "kind": "collectionType",
  "collectionName": "transactions",
  "info": {
    "singularName": "transaction",
    "pluralName": "transactions",
    "displayName": "Transaction",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "amount": {
      "type": "decimal",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "earning",
        "spending",
        "adjusment"
      ],
      "default": "spending",
      "required": true
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "goods": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::good.good",
      "mappedBy": "transaction"
    }
  }
}
