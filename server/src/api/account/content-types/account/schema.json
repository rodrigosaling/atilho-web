{
  "kind": "collectionType",
  "collectionName": "accounts",
  "info": {
    "singularName": "account",
    "pluralName": "accounts",
    "displayName": "Account",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "balance": {
      "type": "decimal"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "checking",
        "savings",
        "credit card"
      ],
      "required": true,
      "default": "checking"
    },
    "owner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::person.person",
      "inversedBy": "accounts"
    }
  }
}
